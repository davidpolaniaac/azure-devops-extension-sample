{
  "name": "azure-devops-extension-sample",
  "version": "1.0.1",
  "description": "Sample Azure DevOps web extension",
  "keywords": [
    "extensions",
    "Azure DevOps",
    "Visual Studio Team Services"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/azure-devops-extension-sample.git"
  },
  "scripts": {
    "build": "npm run compile",
    "build:prod": "npm run compile:prod",
    "compile": "webpack",
    "compile:prod": "webpack --env=production",
    "postbuild": "npm run package",
    "postbuild:prod": "npm run package",
    "package": "tfx extension create --rev-version",
    "gallery-publish": "tfx extension publish --rev-version",
    "clean": "rimraf ./dist && rimraf ./*.vsix"
  },
  "dependencies": {
    "azure-devops-extension-sdk": "^1.141.4",
    "es6-promise": "^4.2.5",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "vss-ui": "^4.134.1"
  },
  "devDependencies": {
    "@types/react": "^16.3.2",
    "@types/react-dom": "^16.0.7",
    "css-loader": "~0.28.7",
    "node-sass": "^4.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "~6.0.6",
    "style-loader": "~0.19.0",
    "tfx-cli": "^0.3.45",
    "ts-loader": "~3.0.3",
    "typescript": "^2.9.2",
    "webpack": "~3.8.1"
  }
}
